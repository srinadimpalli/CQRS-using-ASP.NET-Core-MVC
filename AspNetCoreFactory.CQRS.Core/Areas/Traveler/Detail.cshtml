@model AspNetCoreFactory.CQRS.Core.Areas.Traveler.Detail.Result
@{
    ViewBag.Title = Model.Name;
}

<ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item"><a href="/travelers">Travelers</a></li>
    <li class="breadcrumb-item active">Detail</li>
</ol>

<h4>@Model.Name</h4>

<div class="row p-20">
    <div class="col-12 col-sm-6">
        <div class="form-group form-group-sm row">
            <label for="FirstName" class="col-sm-4 col-form-label">First Name</label>
            <div class="col-sm-8 col-form-text">@Model.FirstName</div>
        </div>
        <div class="form-group form-group-sm row">
            <label for="LastName" class="col-sm-4 col-form-label">Last Name</label>
            <div class="col-sm-8 col-form-text">@Model.LastName</div>
        </div>
        <div class="form-group form-group-sm row">
            <label for="Email" class="col-sm-4 col-form-label">Email</label>
            <div class="col-sm-8 col-form-text">@Model.Email</div>
        </div>
        <div class="form-group form-group-sm row">
            <label for="City" class="col-sm-4 col-form-label">City</label>
            <div class="col-sm-8 col-form-text">@Model.City</div>
        </div>
        <div class="form-group form-group-sm row">
            <label for="Country" class="col-sm-4 col-form-label">Country</label>
            <div class="col-sm-8 col-form-text">@Model.Country</div>
        </div>
        <hr />
        <div class="form-group row">
            <label class="col-sm-4 col-form-label">&nbsp;</label>
            <div class="col-sm-8 col-form-text">
                <a class="btn btn-light btn-sm" href="javascript:window.history.back();">Cancel</a>
            </div>
        </div>
    </div>
</div>
